(
   U  
   psi
   cCation
   cAnion
);

sets
(
    line45deg
    {
        type        face;
        axis        xyz;

        start       ( 0 0 0.5e-5 );
        end         ( 10e-5 10e-5 0.5e-5 );       
    } 
);

