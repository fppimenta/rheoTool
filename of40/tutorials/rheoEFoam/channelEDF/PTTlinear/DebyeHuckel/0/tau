FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    object      tau;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{
    inlet
    {
	type            zeroGradient;     
    }
    
    walls
    {
        type            linearExtrapolation;
	value           uniform (0 0 0 0 0 0);        
    }
    
    symmetry
    {
	type            symmetryPlane;       
    }

    outlet
    {
	type            zeroGradient;      
    }

    frontAndBack
    {
	type            empty;     
    }

}

// ************************************************************************* //
