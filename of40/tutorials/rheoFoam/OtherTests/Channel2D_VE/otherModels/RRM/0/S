FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    object      theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0.333 0 0 0.333 0 0.334);

boundaryField
{
    inlet
    {
	type            fixedValue;
	value           uniform (0.333 0 0 0.333 0 0.334);      
    }
    
    walls
    {
	type            zeroGradient;       
    }

    outlet
    {
	type            zeroGradient;      
    }

    frontAndBack
    {
	type            empty;    
    }
}

