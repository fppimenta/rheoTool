FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [-1 1 1 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    inlet
    {
	type            fixedValue;
	value            uniform 2;
    }
   
    walls
    {
	type            zeroGradient;       
    }

    outlet
    {
	type            zeroGradient;          
    }

    frontAndBack
    {
	type            empty;     
    }
}

