FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
   
    inlet_north
    {
        type            zeroGradient;
    }
    inlet_south
    {
        type            zeroGradient;
    }
    outlet_west
    {
        type            fixedValue;
        value           uniform 0;
    }
     outlet_east
    {
        type            fixedValue;
        value           uniform 0;
    }
    walls
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    
}

