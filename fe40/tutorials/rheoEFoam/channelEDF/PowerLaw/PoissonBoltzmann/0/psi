FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      psi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{

    inletOutlet
    {
	type            cyclic;
    }
    
    symmetry
    {
	type            symmetryPlane;    
    }
   
    walls
    {
	type            fixedValue;
	value           uniform -0.01;         
    }

    frontAndBack
    {
	type            empty;     
    }
    
}

