FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    
    p_inlet
    {
        type            zeroGradient;
    }
    
    p_outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    p_down
    {
        type            zeroGradient;
    }
    
    inter.polymer
    {
        type            zeroGradient;
    }
     
    "(bottom|top)"  
    {
        type             empty;
    }
}

