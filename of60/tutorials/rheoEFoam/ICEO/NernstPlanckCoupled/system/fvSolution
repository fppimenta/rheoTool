// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

coupledSolvers
{
  Uptau
  {
    solveCoupledUp true;
    solveCoupledTau false;
     
    saveSystem true;
    robustSumCheck true;
    updatePrecondFrequency 1000; 
    updateMatrixCoeffs false; 
  }
  
  ciPsi
  {
    solveWithUptau    false;
    
    saveSystem true;
    robustSumCheck  true;
    updatePrecondFrequency 1; 
    updateMatrixCoeffs true;   
  }
}

solvers
{
    "(p|phiE|psi|U)"
    {
        solver          GAMG;
        tolerance       1e-8;
        relTol          0.;
        smoother        DIC;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 10;
        agglomerator    faceAreaPair;
        mergeLevels     1;
        
    }

    "(C|ci)"
    {

        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-9;
        relTol          0;
    }
 
}

