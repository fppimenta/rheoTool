FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          particleProperties;
}

externalFlow
{
    writeFields     false;
    frozenFlow      true;
    tethered        false;
    interpolation   BarycentricWeights;
    gradU           ( 0 0 0 0 0 0 0 0 0 );
    pushBackCmp     ( 0 0 0 );
    pushBackFreq    150;
}

